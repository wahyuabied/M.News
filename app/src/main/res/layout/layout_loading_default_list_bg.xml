<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="8dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="20dp">

        <View
            android:id="@+id/view4"
            android:layout_width="0dp"
            android:layout_height="14dp"
            app:layout_constraintHorizontal_weight="0.35"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space4"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/space4"
            android:layout_width="0dp"
            android:layout_height="14dp"
            app:layout_constraintBottom_toBottomOf="@id/view4"
            app:layout_constraintHorizontal_weight="0.65"
            app:layout_constraintLeft_toRightOf="@id/view4"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view4" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <View
            android:id="@+id/view11"
            android:layout_width="0dp"
            android:layout_height="14dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="24dp"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space11"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/space11"
            android:layout_width="0dp"
            android:layout_height="14dp"
            app:layout_constraintBottom_toBottomOf="@id/view11"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintLeft_toRightOf="@id/view11"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view11" />

        <View
            android:id="@+id/view12"
            android:layout_width="0dp"
            android:layout_height="7dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_weight="0.3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space12"
            app:layout_constraintTop_toBottomOf="@id/view11" />

        <View
            android:id="@+id/space12"
            android:layout_width="0dp"
            android:layout_height="7dp"
            app:layout_constraintBottom_toBottomOf="@id/view12"
            app:layout_constraintHorizontal_weight="0.7"
            app:layout_constraintLeft_toRightOf="@id/view12"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view12" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="24dp"
            app:layout_constraintLeft_toLeftOf="@id/view11"
            app:layout_constraintTop_toBottomOf="@id/view12" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <View
            android:id="@+id/view21"
            android:layout_width="0dp"
            android:layout_height="14dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="24dp"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space21"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/space21"
            android:layout_width="0dp"
            android:layout_height="14dp"
            app:layout_constraintBottom_toBottomOf="@id/view21"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintLeft_toRightOf="@id/view21"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view21" />

        <View
            android:id="@+id/view22"
            android:layout_width="0dp"
            android:layout_height="7dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_weight="0.3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space22"
            app:layout_constraintTop_toBottomOf="@id/view21" />

        <View
            android:id="@+id/space22"
            android:layout_width="0dp"
            android:layout_height="7dp"
            app:layout_constraintBottom_toBottomOf="@id/view22"
            app:layout_constraintHorizontal_weight="0.7"
            app:layout_constraintLeft_toRightOf="@id/view22"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view22" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="24dp"
            app:layout_constraintLeft_toLeftOf="@id/view21"
            app:layout_constraintTop_toBottomOf="@id/view22" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="24dp">

        <View
            android:id="@+id/view31"
            android:layout_width="0dp"
            android:layout_height="14dp"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space31"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/space31"
            android:layout_width="0dp"
            android:layout_height="14dp"
            app:layout_constraintBottom_toBottomOf="@id/view31"
            app:layout_constraintHorizontal_weight="0.5"
            app:layout_constraintLeft_toRightOf="@id/view31"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view31" />

        <View
            android:id="@+id/view32"
            android:layout_width="0dp"
            android:layout_height="7dp"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_weight="0.3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/space32"
            app:layout_constraintTop_toBottomOf="@id/view31" />

        <View
            android:id="@+id/space32"
            android:layout_width="0dp"
            android:layout_height="7dp"
            app:layout_constraintBottom_toBottomOf="@id/view32"
            app:layout_constraintHorizontal_weight="0.7"
            app:layout_constraintLeft_toRightOf="@id/view32"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/view32" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>